generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id        String   @id @default(cuid())
  title     String?
  url       String
  order     Int      @default(0)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Visitor {
  id        String   @id @default(cuid())
  count     Int      @default(0)
  updatedAt DateTime @updatedAt
}
